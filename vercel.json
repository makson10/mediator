{
	"builds": [
		{
			"src": "server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{ "src": "/", "dest": "server.js" },
		{ "src": "/lessons", "dest": "server.js" },
		{ "src": "/hw", "dest": "server.js" },
		{ "src": "/vars", "dest": "server.js" },
		{ "src": "/lessons/(.*)", "dest": "server.js" },
		{ "src": "/hw/(.*)", "dest": "server.js" },
		{ "src": "/vars/(.*)", "dest": "server.js" },
		{ "src": "/history/(.*)", "dest": "server.js" }
	],
	"crons": [
		{
			"path": "/lessons/unpinLessonsScheduleMessage",
			"schedule": "0 14 * * 1-5"
		}
	]
}
